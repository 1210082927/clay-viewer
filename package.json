{
  "name": "qtek-model-viewer",
  "version": "0.1.0",
  "description": "QTEK Model Viewer built on top of WebGL, Supported FBX, DAE, OBJ, glTF2.0 model formats.",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "filer.js": "^0.4.5",
    "glob": "^7.1.2",
    "qtek": "^0.5.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "san": "^3.2.9",
    "webpack": "^3.6.0",
    "zrender": "^3.5.2",
    "shelljs": "^0.7.8"
  },
  "scripts": {
    "build": "npm run glsl2js && rollup -c && uglifyjs -c -m -- dist/QMV.js > dist/QMV.min.js",
    "build:browser": "webpack -p --config editor/webpack.config.js",
    "build:electron": "TARGET=electron webpack --config editor/webpack.config.js",
    "build:fbx2gltf": "pyinstaller editor/electron/convert/fbx2gltf.py && rm -rf editor/electron/convert/build",
    "dist:electron": "npm run build:electron && build",
    "cdist:electron": "export ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ && dist:electron",
    "dev": "rollup -c -w",
    "glsl2js": "node build/glsl2js.js"
  },
  "author": "Yi Shen"
}
